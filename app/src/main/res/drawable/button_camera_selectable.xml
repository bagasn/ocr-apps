<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <stroke android:width="1dp" android:color="#fbf9f9f9" />
                </shape>
            </item>
            <item android:bottom="6dp" android:left="6dp" android:right="6dp" android:top="6dp">
                <shape android:shape="oval">
                    <solid android:color="#fbf1f1f1" />
                </shape>
            </item>
        </layer-list>
    </item>

    <item android:state_enabled="true">
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <stroke android:width="1dp" android:color="#fbf9f9f9" />
                </shape>
            </item>
            <item android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp">
                <shape android:shape="oval">
                    <solid android:color="#fbf9f9f9" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>